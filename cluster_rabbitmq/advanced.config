[{rabbit, [
    {ssl_listeners, [5671]},
    {ssl_options, [
        {certfile, "/etc/rabbitmq/certs/rabbitmq.crt"},
        {keyfile, "/etc/rabbitmq/certs/rabbitmq.key"},
        {cacertfile, "/etc/rabbitmq/certs/rabbitmq.crt"},
        {verify, verify_peer},
        {fail_if_no_peer_cert, true}
    ]}
]}
].
